    "def z1(x,y):\n",
    "    if x<y:\n",
    "        suma=0\n",
    "        while x<y:\n",
    "            y = y-1\n",
    "            if y==x: return suma\n",
    "            suma += y\n",
    "    elif x==y: return x**2\n",
    "    else: return (x-y)"
